<svg viewBox="0 0 200 200" xmlns="http://www.w3.org/2000/svg">
  <!-- Background with gradient -->
  <defs>
    <linearGradient id="bgGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#667eea;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#764ba2;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="pumpGradient" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#ffffff;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#e0e7ff;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="priceGradient" x1="0%" y1="0%" x2="100%" y2="0%">
      <stop offset="0%" style="stop-color:#10b981;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#34d399;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Rounded square background -->
  <rect width="200" height="200" rx="40" fill="url(#bgGradient)"/>
  
  <!-- Location pin circle -->
  <circle cx="100" cy="85" r="45" fill="#ffffff" opacity="0.2"/>
  
  <!-- Main location pin -->
  <path d="M 100 50 C 85 50 73 62 73 77 C 73 92 100 120 100 120 C 100 120 127 92 127 77 C 127 62 115 50 100 50 Z" 
        fill="url(#pumpGradient)" stroke="#667eea" stroke-width="2"/>
  
  <!-- Gas pump icon inside pin -->
  <g transform="translate(100, 77)">
    <!-- Pump body -->
    <rect x="-8" y="-12" width="16" height="18" rx="2" fill="#667eea"/>
    <!-- Pump display -->
    <rect x="-6" y="-9" width="12" height="6" rx="1" fill="#34d399"/>
    <!-- Nozzle -->
    <path d="M 8 -3 L 14 -3 L 14 0 L 11 0 L 11 3" stroke="#667eea" stroke-width="2" fill="none" stroke-linecap="round"/>
    <!-- Pump base -->
    <rect x="-8" y="6" width="16" height="3" fill="#764ba2"/>
  </g>
  
  <!-- Price tag badge -->
  <g transform="translate(145, 135)">
    <rect x="-28" y="-18" width="56" height="36" rx="18" fill="url(#priceGradient)" opacity="0.95"/>
    <text x="-5" y="8" text-anchor="middle" font-family="Arial, sans-serif" font-size="24" font-weight="bold" fill="#ffffff">â‚©</text>
    <circle cx="15" cy="-5" r="4" fill="#ffffff"/>
    <path d="M 11 -9 L 19 -1" stroke="#ffffff" stroke-width="2.5" stroke-linecap="round"/>
  </g>
  
  <!-- Navigation indicator dots -->
  <circle cx="60" cy="145" r="4" fill="#ffffff" opacity="0.6"/>
  <circle cx="75" cy="150" r="3" fill="#ffffff" opacity="0.4"/>
  <circle cx="90" cy="153" r="2" fill="#ffffff" opacity="0.3"/>
</svg>